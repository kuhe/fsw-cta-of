{entitymanager
	{group "dead" actor dead
		(mod "mp" 		{limit 50})
		(mod not "mp"	{limit 200})
		{remove dissolve}
		{entity "human"}
	}

	{group "broken" gameentity
		(mod "mp" 		{limit 30})
		(mod not "mp"	{limit 60})
		{remove dissolve}
		{DeadVehicles}
	}

	{group "piece" gameentity
		(mod "mp" 		{limit 50})
		(mod not "mp"	{limit 80})
		{remove dissolve}
		{entity "piece_big_steel"}				;// cannon parts
		{entity "piece_large_steel"}			;// tank turret, ship parts
		{entity "piece_big_airborne_wing"}		;// airborne parts
	}

	{group "piece_medium" gameentity
		(mod "mp" 		{limit 50})
		(mod not "mp"	{limit 80})
		{remove dissolve}
		{entity "piece_medium_wood"}			;// fences
		{entity "piece_medium_steel"}			;// vehicle parts and wheels
	}

	{group "stuff" stuff
		(mod "mp" 		{limit 100})
		(mod not "mp"	{limit 200})
		{remove dissolve}
		{tag "shell"}
		{tag "weapon bullet"}
		{tag "grenade"}
		{tag "missile"}
		{tag "sharp_edge"}
	}

	{group "equip" stuff
		(mod "mp" 		{limit 30})
		(mod not "mp"	{limit 60})
		{remove dissolve}
		{tag "field_glass"}
		{tag "head"}
		{tag "body"}
	}

	{group "shellhole" gameentity
		(mod "mp" 		{limit 50})
		(mod not "mp"	{limit 100})
		{remove instant}
		{entity "grenade_shellhole"}
		{entity "grenade_at_shellhole"}
	}
	
	{group "shell_auto" stuff
		{limit 100}
		{remove dissolve}
		{tag "shell_cannon_auto"}
	}

	{group "shell" stuff
		{limit 100}
		{remove dissolve}
		{tag "shell_cannon"}
	}
}
